<div class="wrap-collabsible">
    <input id="<%=id  %>" class="toggle" type="checkbox" <% if(index===0) { %> checked <% } %>>
        <label for="<%=id  %>" class="lbl-toggle">
            <svg class="indicator" width="16" height="10" viewBox="0 0 16 10" fill="none"
                xmlns="http://www.w3.org/2000/svg">
                <path
                    d="M14.1003 9.59961L7.98895 3.66593L1.87761 9.59961L0 7.77285L7.98895 -0.000391006L15.9779 7.77285L14.1003 9.59961Z"
                    fill="black" />
                <path
                    d="M7.95412 3.63006L7.98895 3.59624L8.02378 3.63006L14.1002 9.52988L15.9062 7.77285L7.98895 0.0693722L0.0717001 7.77285L1.87765 9.52988L7.95412 3.63006Z"
                    stroke="black" stroke-opacity="0.4" stroke-width="0.1" />
            </svg>
            <%= title %>
        </label>
        <div class="collapsible-content">
            <div class="content-inner">
                <p>
                    <%= content %>
                </p>
                <div class="readMore">See More</div>
            </div>
        </div>
</div>